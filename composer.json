{
    "name": "pboese/laravelchartjs",
    "version": "3.0.0",
    "description": "Simple package to facilitate and automate the use of charts in Laravel 8.x using Chartjs v2 library. Forked from https://github.com/fxcosta/laravel-chartjs",
    "keywords": [
      "chartjs","laravel8","chart","reports","graphics", "fx3costa"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pelle Boese",
            "email": "pelle@boe.se"
        }
    ],
    "require": {
      "php": ">=5.6.4",
      "illuminate/support": "^5.1|^6.0|^7.0|^8.0"
    },

    "autoload": {
      "psr-4": {
        "Pboese\\LaravelChartJs\\" : "src/"
      }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Pboese\\LaravelChartJs\\Providers\\ChartjsServiceProvider"
            ]
        }
    }
}
